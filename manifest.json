{
    "name": "Chess",
    "version": "3.0",
    "manifest_version": 3,
    "description": "Premium cheat v3.0",
    "content_scripts": [
        {
            "matches": ["*://*.lichess.org/*"],
            "js": ["js/lichess.js"]
        },
		{
			"matches": ["*://*.chess.com/*"],
			"js": ["js/chess.js"]
		}
    ],
    "icons": {
        "16": "icon/knight.png",
        "32": "icon/knight.png",
        "48": "icon/knight.png",
        "128": "icon/knight.png"
    }
}